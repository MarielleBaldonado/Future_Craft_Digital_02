                        </div>
                        <div class="carousel-item">
                            <img src="~/Assets/img/rental/image4.jpeg" alt="Image 2">
                        </div>
                        <div class="carousel-item">
                            <img src="~/Assets/img/rental/image7.jpeg" alt="Image 3">
                        </div>
                        <div class="carousel-item">
                            <img src="~/Assets/img/rental/image6.jpeg" alt="Image 4">
                        </div>
                        <div class="carousel-item">
                            <img src="~/Assets/img/rental/image8.png" alt="Image 5">
                        </div>
                    </div>
                    <ol class="carousel-indicators">
                        <li data-bs-target="#products-details-carousel" data-bs-slide-to="0" class="active"></li>
                        <li data-bs-target="#products-details-carousel" data-bs-slide-to="1"></li>
                        <li data-bs-target="#products-details-carousel" data-bs-slide-to="2"></li>

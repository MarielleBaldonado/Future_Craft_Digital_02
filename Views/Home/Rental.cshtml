                    <a class="carousel-control-next" href="#products-details-carousel" role="button" data-bs-slide="next">
                        <span class="carousel-control-next-icon" aria-hidden="true"></span>
                        <span class="visually-hidden">Next</span>
                    </a>
                </div>
            </div>

            <div class="col-lg-6">
                <div class="ptb-80">
